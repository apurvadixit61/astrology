<script type="text/javascript" async="async">
  $(document).ready(function () {
    $(function () {
      $("img.lazy").lazyload({effect: "fadeIn", failure_limit: 50});
    }), $('img').error(function () {
      $(this).attr('src', 'https://static.indiatvnews.com/ins-web/images/lazy.jpg');
    });
  });
  setTimeout(function () {
    breaking_now();
  }, 60000);
  function breaking_now() {
    $.ajax({
      type: 'GET',
      async: true,
      url: "/ajax/checkbreakingnow/q",
      success: function (data) {
        if (data != '') {
          if ($('#newslist').length > 0) {
            $('#newslist').vTicker();
            $('#breaking-ul-update').html(data);
          }
          $('#breakingNow').show();
        } else {
          $('#breakingNow').hide();
        }
      }
    });
    setTimeout(function () {
      breaking_now();
    }, 60000);
  }
  ;</script>
